[
    {
        "name": "Manage faq articles",
        "slug": "faq-manage-all",
        "description": "Allows user to perform all actions on FAQ app",
        "permissions": [
            {
                "classId": "Faq.Entities.Article",
                "rules": {
                    "c": true,
                    "u": true,
                    "d": true,
                    "r": true,
                    "category/{slug}": {
                        "get": true
                    }
                }
            },
            {
                "classId": "Faq.Entities.Category",
                "rules": {
                    "c": true,
                    "u": true,
                    "d": true,
                    "r": true
                }
            }
        ]
    }
]